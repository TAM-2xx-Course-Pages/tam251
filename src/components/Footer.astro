    footer .il-footer-social li a > svg{
        height: 20px;
        fill: var(--illini-industrial-blue);
    }

    footer .il-footer-social ul {
        display: flex;
        flex-wrap: wrap;
    }

    footer .il-footer-social li {
        margin-right: 1rem;
    }

    footer ul {
        list-style-type: none;
        padding: 0;
        margin: 0;
    }

    .button.orange {
        background-color: var(--illini-orange) !important;
        border-color: var(--illini-orange) !important;
        color: white !important;
    }

    footer .button {
        margin: 0 0.5rem 0.5rem 0;
    }

    .button {
        border: 1px solid var(--engr-royal);
        background-color: transparent;
        color: var(--engr-royal);
        font-weight: 900;
        display: inline-block;
        line-height: 1.3;
        text-align: center;
        padding: 10px 2rem;
        text-decoration: none;
        margin: 0.25rem;
        font-size: 1.2rem;
    }

    @media (min-width: 768px) {
        .text-md-right {
            text-align: right !important;
        }
    }

    footer .campus-footer .campus-wordmark a {
        display: block;
        margin-bottom: 60px;
        max-width: 151px;
    }

    footer .campus-footer h3 {
        font-size: 1.5rem;
        font-weight: 600;
    }

    footer .campus-footer .links li {
        padding: 0.5rem 0;
    }
    footer .campus-footer .links a {
        text-decoration: none;
        color: var(--engr-white);
        display: inline-block;
    }

    footer .legal-footer {
        border-top: 8px solid var(--illini-industrial-blue-darker-2);
        padding-top: 36px;
        padding-bottom: 36px;
        background-color: var(--illini-blue);
        color: #ffffff;
    }

    footer .legal-footer ul {
        display: flex;
        flex-wrap: wrap;
        align-items: center;
        gap: 30px;
        min-height: 60px;
    }

    footer .legal-footer a {
        color: var(--engr-white);
        text-decoration: underline;
        padding: 5px;
    }

    footer .legal-footer #ot-sdk-btn.ot-sdk-show-settings {
        border: 1px solid white !important;
        color: white;
        border-radius: 5px;
        font-size: 14px;
        line-height: 1.2;
        background-color: var(--illini-alma-blue) !important;
        padding: 11px 16px !important;
        text-decoration: none;
        font-family: inherit !important;
    }
</style>

